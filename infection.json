{
  "source": {
    "directories": [
      "src/FachowyWarsztat/Module",
      "src/FachowyWarsztat/Infrastructure"
    ],
    "excludes": [
      "{.*/Infrastructure/Repository/.*}",
      "{.*/Exception/.*}",
      "{.*/API/.*}",
      "{.*/DTO/.*}",
      "{.*/Command/.*}"
    ]
  },
  "logs": {
    "text": "infection.log",
    "perMutator": "per-mutator.md"
  },
  "mutators": {
    "@default": true,
    "@function_signature": false
  },
  "minMsi": 50,
  "minCoveredMsi": 50,
  "testFramework":"phpunit"
}
