{
  "source": {
    "directories": [
      "src"
    ],
    "excludes": [
      "{.*/Repository/.*}",
      "{.*/Exception/.*}",
      "{.*/Entity/.*}",
      "{.*/DTO/.*}",
      "{.*/Command/.*}",
      "{.*/Controller/.*}"
    ]
  },
  "logs": {
    "text": "infection.log",
    "perMutator": "per-mutator.md"
  },
  "mutators": {
    "@default": true,
    "@function_signature": false
  },
  "minMsi": 50,
  "minCoveredMsi": 50,
  "testFramework":"phpunit"
}
